{
  "appLabel": "SweetLink Example",
  "appUrl": "http://localhost:3000",
  "prodUrl": "https://sweetistics.com",
  "daemonUrl": "https://localhost:4455",
  "oauthScript": "apps/sweetlink/examples/oauth/twitter-oauth-automation.ts",
  "healthChecks": {
    "paths": ["/api/health"]
  },
  "cookieMappings": [
    {
      "hosts": ["sweetistics.com", "*.sweetistics.com", "localhost", "127.0.0.1"],
      "origins": [
        "https://sweetistics.com",
        "https://www.sweetistics.com",
        "https://app.sweetistics.com",
        "https://auth.sweetistics.com",
        "https://x.com",
        "https://twitter.com",
        "https://api.twitter.com"
      ]
    }
  ],
  "servers": [
    {
      "env": "dev",
      "start": ["./runner", "pnpm", "run", "dev"],
      "check": ["curl", "--fail", "http://localhost:3000/api/health"],
      "timeoutMs": 60000
    }
  ],
  "smokeRoutes": {
    "defaults": [
      "timeline/home",
      "insights",
      "search",
      "",
      "pulse",
      "settings/account",
      "settings/activity",
      "settings/billing",
      "settings/notifications",
      "settings/social",
      "settings/sync",
      "settings/import",
      "settings/extension",
      "settings/beta"
    ],
    "presets": {}
  }
}
