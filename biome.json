          }
        }
      }
    },
    {
      "includes": ["tests/**", "src/mocks/**"],
      "linter": {
        "rules": {
          "performance": {
            "noAwaitInLoops": "off"
          },
          "style": {
            "noCommonJs": "off",
            "noSubstr": "off",
            "useReadonlyClassProperties": "off",
            "noRestrictedImports": "off"
          }
        }
      }
    },
    {
      "includes": ["src/lib/twitter/**"],
      "linter": {
        "rules": {
          "style": {
            "noRestrictedImports": "off"
          }
        }
      }
    },
    {
      "includes": ["packages/**", "scripts/**"],
      "linter": {
        "rules": {
          "performance": {
            "noAwaitInLoops": "off"
          },
          "style": {
            "useReadonlyClassProperties": "off"
          }
        }
      }
    },
    {
      "includes": ["src/app/api/**", "src/lib/**"],
      "linter": {
        "rules": {
          "nursery": {
            "noUnresolvedImports": "off"
          }
        }
      }
    },
    {
      "includes": ["tests/**", "src/mocks/**", "src/types/**"],
      "linter": {
        "rules": {
          "nursery": {
            "noUnresolvedImports": "off",
            "noImportCycles": "off"
          }
        }
      }
    },
    {
      "includes": ["packages/**", "scripts/**", "apps/**", "tests/**", "src/mocks/**"],
      "linter": {
        "rules": {
          "correctness": {
            "noUnusedImports": "off"
          },
