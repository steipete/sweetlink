{
  "$schema": "https://biomejs.dev/schemas/2.3.10/schema.json",
  "extends": [
    "ultracite/core",
    "ultracite/react",
    "ultracite/next"
  ],
  "vcs": {
    "enabled": true,
    "clientKind": "git",
    "useIgnoreFile": true
  },
  "javascript": {
    "formatter": {
      "quoteStyle": "single",
      "semicolons": "always",
      "trailingCommas": "es5"
    }
  },
  "json": {
    "formatter": {
      "indentStyle": "space",
      "indentWidth": 2
    }
  },
  "files": {
    "ignoreUnknown": true,
    "maxSize": 2097152,
    "includes": [
      "**",
      "!**/_generated/**/*",
      "!**/*.gen.*",
      "!**/.next/**/*",
      "!**/next-env.d.ts",
      "!**/.nuxt/**/*",
      "!**/.output/**/*",
      "!**/.svelte-kit/**/*",
      "!**/.vitepress/cache/**/*",
      "!**/.vitepress/dist/**/*",
      "!**/dist/**/*",
      "!**/build/**/*",
      "!**/out/**/*",
      "!**/.turbo/**/*",
      "!**/.vercel/**/*",
      "!**/.netlify/**/*",
      "!**/storybook-static/**/*",
      "!**/.docusaurus/**/*",
      "!**/.cache/**/*",
      "!**/public/build/**/*",
      "!**/.parcel-cache/**/*",
      "!**/.vite/**/*",
      "!**/.astro/**/*",
      "!**/_astro/**/*",
      "!**/coverage/**/*",
      "!**/.nyc_output/**/*",
      "!**/*.generated.*",
      "!**/*.auto.*",
      "!**/generated/**/*",
      "!**/auto-generated/**/*",
      "!**/codegen/**/*",
      "!**/__generated__/**/*",
      "!**/graphql-types.*",
      "!**/schema.d.ts",
      "!**/schema.graphql.d.ts",
      "!**/*.d.ts.map",
      "!**/.expo/**/*",
      "!**/.expo-shared/**/*",
      "!**/android/build/**/*",
      "!**/ios/build/**/*",
      "!**/DerivedData/**/*",
      "!**/apps/client/src-tauri/gen",
      "!**/apps/desktop",
      "!**/public/mockServiceWorker.js",
      "!**/.jscpd-report",
      "!**/reports",
      "!scripts/cli/iterm-bundle.js",
      "!!src/runtime/browser/selector-discovery.ts"
    ]
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space",
    "indentWidth": 2,
    "lineWidth": 120
  },
  "css": {
    "parser": {
      "tailwindDirectives": true
    }
  },
  "linter": {
    "enabled": true,
    "domains": {
      "next": "recommended",
      "react": "recommended",
      "project": "recommended",
      "test": "recommended"
    },
    "rules": {
      "recommended": true,
      "a11y": {
        "recommended": true,
        "useAnchorContent": "error",
        "useHeadingContent": "error",
        "useHtmlLang": "error",
        "useFocusableInteractive": "error",
        "noSvgWithoutTitle": "warn",
        "useButtonType": "error",
        "useValidAnchor": "error",
        "useKeyWithClickEvents": "error",
        "noStaticElementInteractions": "error",
        "useValidAriaProps": "error",
        "useAltText": "error",
        "noRedundantAlt": "warn",
        "noNoninteractiveTabindex": "warn",
        "noNoninteractiveElementToInteractiveRole": "warn",
        "useAriaPropsForRole": "warn",
        "noAutofocus": "warn"
      },
      "style": {
        "noUnusedTemplateLiteral": "error",
        "useConst": "error",
        "useTemplate": "error",
        "noCommonJs": "error",
        "noSubstr": "error",
        "useDefaultParameterLast": "error",
        "useNodejsImportProtocol": "off",
        "noProcessEnv": "error",
        "noRestrictedGlobals": "error",
        "useCollapsedElseIf": "warn",
        "useSingleVarDeclarator": "warn",
        "noUselessElse": "warn",
        "useAsConstAssertion": "warn",
        "useForOf": "warn",
        "noNegationElse": "warn",
        "useArrayLiterals": "error",
        "noRestrictedImports": {
          "level": "error",
          "options": {
            "paths": {
              "twitter-api-v2": {
                "importNames": ["TwitterApi"],
                "message": "Never import TwitterApi directly. Use TwitterService.forCurrentUser()/forUser()."
              },
              "@sweetistics/trpc-server/adapters/next": {
                "message": "Route Handlers must use fetchRequestHandler from @sweetistics/trpc-server."
              }
            }
          }
        },
        "useBlockStatements": "off",
        "useFragmentSyntax": "warn",
        "useConsistentTypeDefinitions": "off",
        "useFilenamingConvention": "off",
        "useShorthandFunctionType": "error",
        "useImportType": "error",
        "useExportType": "error",
        "useConsistentArrayType": "error",
        "noParameterAssign": "error"
      },
      "correctness": {
        "noUnusedVariables": "error",
        "noUnusedImports": "error",
        "noNestedComponentDefinitions": "error",
        "useUniqueElementIds": "warn",
        "useExhaustiveDependencies": "error",
        "noChildrenProp": "error",
        "noVoidElementsWithChildren": "error",
        "useHookAtTopLevel": "error",
        "noUnreachable": "error",
        "noConstantCondition": "warn",
        "useJsxKeyInIterable": "error",
        "noRenderReturnValue": "error"
      },
      "suspicious": {
        "noDocumentCookie": "warn",
        "noExplicitAny": "error",
        "noArrayIndexKey": "error",
        "noDuplicateObjectKeys": "error",
        "noUnknownAtRules": "off",
        "noDoubleEquals": "error",
        "noUnsafeDeclarationMerging": "error",
        "noExtraNonNullAssertion": "error",
        "noDocumentImportInPage": "error",
        "noHeadImportInDocument": "error",
        "noCommentText": "error",
        "noDuplicateJsxProps": "error",
        "noSuspiciousSemicolonInJsx": "error",
        "noSelfCompare": "error",
        "noAssignInExpressions": "warn",
        "noGlobalAssign": "error",
        "noImportAssign": "error",
        "noFallthroughSwitchClause": "error",
        "noShadowRestrictedNames": "warn",
        "noVar": "error",
        "useIsArray": "error",
        "noConfusingVoidType": "warn",
        "noPrototypeBuiltins": "warn",
        "noConsole": {
          "level": "warn",
          "options": {
            "allow": ["error", "warn", "info", "log"]
          }
        },
        "noNonNullAssertedOptionalChain": "error"
      },
      "security": {
        "noDangerouslySetInnerHtml": "warn",
        "noDangerouslySetInnerHtmlWithChildren": "error",
        "noGlobalEval": "error",
        "noBlankTarget": "error",
        "noSecrets": "off"
      },
      "performance": {
        "noImgElement": "warn",
        "noAccumulatingSpread": "warn",
        "noUnwantedPolyfillio": "error",
        "useGoogleFontPreconnect": "info",
        "noAwaitInLoops": "off",
        "noDelete": "warn",
        "noReExportAll": "off"
      },
      "complexity": {
        "noExcessiveCognitiveComplexity": {
          "level": "warn",
          "options": {
            "maxAllowedComplexity": 50
          }
        },
        "noImportantStyles": "off",
        "noStaticOnlyClass": "off",
        "useFlatMap": "warn",
        "noBannedTypes": "error",
        "useRegexLiterals": "warn",
        "noUselessFragments": "warn",
        "noForEach": "warn",
        "useArrowFunction": "warn",
        "useOptionalChain": "error",
        "noUselessRename": "error",
        "useLiteralKeys": "warn",
        "noUselessTernary": "error",
        "useSimpleNumberKeys": "warn",
        "noUselessCatch": "warn",
        "noUselessConstructor": "error"
      },
      "nursery": {
        "noUnnecessaryConditions": "warn",
        "noFloatingPromises": "error",
        "noNextAsyncClientComponent": "warn",
        "noUnresolvedImports": "off",
        "noImportCycles": "warn",
        "noMisusedPromises": "warn",
        "noShadow": "warn",
        "useSortedClasses": "off"
      }
    }
  },
  "assist": {
    "enabled": true,
    "actions": {
      "source": {
        "organizeImports": "on",
        "useSortedAttributes": "off"
      }
    }
  },
  "overrides": [
    {
      "includes": [
        "*.config.{js,ts,mjs,cjs,mts,cts}",
        "vite.config.*",
        "vitest.config.*",
        "jest.config.*",
        "rollup.config.*",
        "webpack.config.*",
        "eslint.config.*",
        "prettier.config.*",
        "stylelint.config.*",
        "commitlint.config.*",
        "lint-staged.config.*",
        "postcss.config.*",
        "tailwind.config.*",
        "playwright.config.*",
        "cypress.config.*",
        "babel.config.*",
        "metro.config.*",
        "next.config.*",
        "nuxt.config.*",
        "astro.config.*",
        "svelte.config.*",
        "tsup.config.*"
      ]
    },
    {
      "includes": ["**/*.{test,spec}.{ts,tsx,js,jsx}", "**/__tests__/**/*"],
      "linter": {
        "rules": {
          "complexity": {
            "noExcessiveCognitiveComplexity": "off"
          },
          "suspicious": {
            "noConsole": "off",
            "noExplicitAny": "off"
          }
        }
      }
    },
    {
      "includes": ["**/scripts/**/*", "**/bin/**/*", "*.mjs", "*.cjs"],
      "linter": {
        "rules": {
          "suspicious": {
            "noConsole": "off"
          },
          "style": {
            "noProcessEnv": "off"
          }
        }
      }
    },
    {
      "includes": ["**/*.{stories,story}.{ts,tsx,js,jsx}"],
      "linter": {
        "rules": {
          "correctness": {
            "noUnusedVariables": "off",
            "noUnusedImports": "off"
          }
        }
      }
    },
    {
      "includes": ["**/*.d.ts", "**/{env,global,types}.d.ts"],
      "linter": {
        "rules": {
          "correctness": {
            "noUnusedVariables": "off",
            "noUndeclaredVariables": "off"
          }
        }
      }
    },
    {
      "includes": [
        "**/dist/**/*",
        "**/build/**/*",
        "**/.next/**/*",
        "**/.nuxt/**/*",
        "**/out/**/*",
        "**/coverage/**/*",
        "**/*.min.js",
        "**/*.min.css",
        "**/*.md",
        "**/*.mdx",
        "**/*.graphql",
        "**/*.gql",
        ".env",
        ".env.*"
      ],
      "formatter": {
        "enabled": false
      },
      "linter": {
        "enabled": false
      }
    },
    {
      "includes": [
        "**/package.json",
        "**/{tsconfig,jsconfig}.json",
        "**/tsconfig.*.json",
        ".vscode/*.json",
        ".github/**/*.json"
      ],
      "json": {
        "parser": {
          "allowComments": true,
          "allowTrailingCommas": true
        }
      }
    },
    {
      "includes": [
        "packages/**",
        "scripts/**",
        "apps/**",
        "public/**",
        "**/*.config.*",
        "**/__tests__/**",
        "**/*.test.*",
        "**/*.spec.*",
        "src/mocks/**"
      ],
      "linter": {
        "rules": {
          "nursery": {
            "noShadow": "off"
          }
        }
      }
    },
    {
      "includes": [
        "src/lib/logger-core.node.ts",
        "src/lib/logger-node.ts",
        "src/lib/request-context.ts",
        "src/lib/inngest/client.ts"
      ],
      "linter": {
        "rules": {
          "style": {
            "noCommonJs": "off"
          },
          "complexity": {
            "noExcessiveCognitiveComplexity": "off"
          }
        }
      }
    },
    {
      "includes": ["apps/**"],
      "linter": {
        "rules": {
          "style": {
            "noCommonJs": "off"
          }
        }
      }
    },
    {
      "includes": ["apps/**/metro.config.js", "**/*.config.js"],
      "linter": {
        "rules": {
          "style": {
            "noCommonJs": "off"
          }
        }
      }
    },
    {
      "includes": ["src/lib/validation/transformers.ts"],
      "linter": {
        "rules": {
          "performance": {
            "noReExportAll": "off"
          }
        }
      }
    },
    {
      "includes": ["src/**"],
      "linter": {
        "rules": {
          "performance": {
            "noAwaitInLoops": "error"
          }
        }
      }
    },
    {
      "includes": [
        "src/lib/archive/**",
        "src/lib/hooks/**",
        "src/lib/inngest/**",
        "src/lib/friendimizer.ts",
        "src/lib/db-helpers/user-sync-helpers.ts",
        "src/lib/link-metadata-cache.ts",
        "src/lib/streaming/**",
        "src/lib/sync/**",
        "src/lib/turbopuffer*.ts",
        "src/lib/timeline-snapshots/**",
        "src/lib/tweet-search.ts",
        "src/lib/twitter/**",
        "src/app/api/user/**",
        "src/app/api/replies/route.ts",
        "src/app/api/sync/trigger/route.ts"
      ],
      "linter": {
